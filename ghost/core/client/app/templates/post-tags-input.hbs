
<ul class="tag-list">
    <li class="label"><h4>Tag your story:</h4></li>
    {{#each tag in controller.displayTagNames}}
        <li class="tag">
            {{#if (gh-in-array tag controller.tags)}}
                <a href="#" class="active" {{action "toggleTag" tag  }}>{{tag}}</a>
            {{else}}
                <a href="#" {{action "toggleTag" tag  }}>{{tag}}</a>
            {{/if}}
        </li>
    {{/each}}
</ul>
